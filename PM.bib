@article{ ISI:000306586800035,
Author = {Sherman, Craig D. H. and Stanley, Annalise M. and Keough, Michael J. and
   Gardner, Michael G. and Macreadie, Peter I.},
Title = {{Development of twenty-three novel microsatellite markers for the
   seagrass, Zostera muelleri from Australia}},
Journal = {{CONSERVATION GENETICS RESOURCES}},
