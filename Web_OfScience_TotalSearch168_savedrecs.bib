@article{ ISI:000365070700050,
Author = {Davis, Jenny L. and Currin, Carolyn A. and O'Brien, Colleen and
   Raffenburg, Craig and Davis, Amanda},
Title = {{Living Shorelines: Coastal Resilience with a Blue Carbon Benefit}},
Journal = {{PLOS ONE}},
Year = {{2015}},
