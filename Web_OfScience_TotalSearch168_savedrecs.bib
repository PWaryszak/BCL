@article{ ISI:000380117300012,
Author = {Kerr, Drew W. and Hogle, Ingrid B. and Ort, Brian S. and Thornton,
   Whitney J.},
Title = {{A review of 15 years of Spartina management in the San Francisco Estuary}},
