@article{ ISI:000440452500014,
Author = {Davenport, Theresa M. and Seitz, Rochelle D. and Knick, Kathleen E. and
   Jackson, Nina},
Title = {{Living Shorelines Support Nearshore Benthic Communities in Upper and
   Lower Chesapeake Bay}},
