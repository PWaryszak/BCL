@article{ ISI:000398772900006,
Author = {Wigand, Cathleen and Ardito, Thomas and Chaffee, Caitlin and Ferguson,
   Wenley and Paton, Suzanne and Raposa, Kenneth and Vandemoer, Charles and
   Watson, Elizabeth},
Title = {{A Climate Change Adaptation Strategy for Management of Coastal Marsh
   Systems}},
