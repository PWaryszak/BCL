@article{ ISI:000448508400005,
Author = {Polk, Mariko A. and Eulie, Devon O.},
Title = {{Effectiveness of Living Shorelines as an Erosion Control Method in North
   Carolina}},
Journal = {{ESTUARIES AND COASTS}},
Year = {{2018}},
Volume = {{41}},
